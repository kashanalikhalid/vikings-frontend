    "gulp-append-prepend": "1.0.8",
    "moment": "^2.29.1",
    "mongoose": "^5.13.7",
    "node-sass": "4.14.1",
    "qrcode": "^1.4.4",
    "qrcode.react": "^1.0.1",
    "react": "17.0.1",
    "react-bootstrap": "1.4.3",
